@page
@model WebForm.Pages.Shared.ThongKeModel
@{
    ViewData["Title"] = "Website Quản Lý Tour";
    Layout = "_Layout";
}

<div class="main-panel">
    <div class="content-wrapper">
        <div class="row">
            <div class="col-12 grid-margin stretch-card">
                <div class="card">
                    <div class="card-body">
                        <h4 class="card-title">Thống kê</h4>
                        <input type="text" class="form-control mb-2 mr-sm-2" style="margin-left:" placeholder="Thời gian bắt đầu">
                        <input type="text" class="form-control mb-2 mr-sm-2" style="margin-left:" placeholder="Thời gian kết thúc">
                        <form class="forms-sample" method="post">
                            <div class="form-group">
                                <label for="exampleInputName1">Chi phí khách sạn</label>
                                <input type="text" class="form-control" placeholder="Chi phí khách sạn" readonly>
                            </div>
                            <div class="form-group">
                                <label for="exampleInputName1">Ăn uống</label>
                                <input type="text" class="form-control" placeholder="Ăn uống" readonly>
                            </div>
                            <div class="form-group">
                                <label for="exampleInputName1">Phương tiện</label>
                                <input type="text" class="form-control" placeholder="Phương tiện" readonly>
                            </div>
                            <div class="form-group">
                                <label for="exampleInputName1">Doanh số</label>
                                <input type="text" class="form-control" placeholder="Doanh số" readonly>
                            </div>
                            <div class="form-group">
                                <label for="exampleInputName1">Số Đoàn tham gia</label>
                                <input type="text" class="form-control" placeholder="Số Đoàn tham gia" readonly>
                            </div>
                            <div class="form-group">
                                <label for="exampleInputName1">Tổng</label>
                                <input type="text" class="form-control" placeholder="Tổng" readonly>
                            </div>
                            <input type="submit" value="Tìm" class="btn btn-primary mr-2" />
                            <a class="btn btn-light">Hủy</a>
                        </form>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>